{
  "extends": ["tslint:latest", "tslint-eslint-rules", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["docs/**/*"]
  },
  "rules": {
    "member-access": false,
    "ordered-imports": false,
    "no-console": { "severity": "warning" },
    "object-literal-sort-keys": false,
    "no-submodule-imports": false,
    "no-namespace": false,
    "no-implicit-dependencies": { "severity": "warning" },
    "completed-docs": true,
    "variable-name": [true, "allow-leading-underscore", "allow-pascal-case"]
  }
}
